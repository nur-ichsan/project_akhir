<section class="page-section" id="services">
  <div class="container">
    <div class="text-center">
      <h2 class="section-heading text-uppercase">Six Key Arabic Braille</h2>
      <h3 class="section-subheading text-muted">
        Pengetikan Arab dengan Braille Enam Tombol
      </h3>
    </div>
    <form action="" accept-charset="ISO-8859-1">
      <div class="row align-items-stretch mb-12">
        <div class="col-md-12">
          <div class="form-group form-group-textarea mb-md-9">
            <label for="brl">Braille QWERTY Mode</label>
            <textarea
              class="form-control"
              name="brl"
              id="brl"
              tabindex="1"
              placeholder="Your Message *"
              required="required"
              data-validation-required-message="Please enter a message."
            ></textarea>
            <p class="help-block text-danger"></p>
          </div>

          <div class="form-group"></div>

          <div class="form-group mb-md-9"></div>
        </div>

        <div class="col-md-12">
          <div class="form-group form-group-textarea mb-md-9">
            <label for="awas">PRINT TEXT</label>
            <textarea
              class="form-control"
              name="awas"
              id="awas"
              tabindex="2"
              placeholder="Your Message *"
              required="required"
              data-validation-required-message="Please enter a message."
            ></textarea>
            <p class="help-block text-danger"></p>
          </div>

          <div class="form-group"></div>

          <div class="form-group mb-md-9"></div>
        </div>

        <div class="col-md-12">
          <div class="form-group form-group-textarea mb-md-9">
            <label for="unicode">Arabic Unicode</label>
            <textarea
              class="form-control"
              name="unicode"
              id="unicode"
              tabindex="3"
              placeholder="Your Message *"
              required="required"
              data-validation-required-message="Please enter a message."
            ></textarea>
            <p class="help-block text-danger"></p>
          </div>

          <div class="form-group"></div>

          <div class="form-group mb-md-9"></div>
        </div>
      </div>

      <div>
        <input type="reset" value="reset" />
        <input type="checkbox" name="sixkey" id="sixkey" value="1" /><label
          for="sixkey"
          >SIXKEY Mode</label
        >
      </div>
    </form>
  </div>
</section>

<script src="../assets/js/QrBraille.js"></script>
